<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="group_change_inventory_date" model="res.groups">
            <field name="name">Allow to change inventory date</field>
            <field name="category_id" ref="base.module_category_hidden" />
        </record>
        <record id="group_view_inventory_button" model="res.groups">
            <field name="name">Can update quantities</field>
            <field name="category_id" ref="base.module_category_hidden" />
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
        </record>
        <record id="group_merge_inventory" model="res.groups">
            <field name="name">Can merge inventory documents</field>
            <field name="category_id" ref="base.module_category_hidden" />
        </record>
    </data>
</odoo>
