<?xml version="1.0" encoding="utf-8"?><openerp>    <data>        <record id="view_mrp_order_merge_form" model="ir.ui.view">            <field name="name">mrp.order.mergeform</field>            <field name="model">mrp.order.merge</field>            <field name="arch" type="xml">                <form>                    <group string="Merge Production Order">                        <div>                            Select Production Order                        </div>                    </group>                    <field name="production_ids"/>                    <footer>                        <button name="do_merge" string="_Apply" type="object" class="oe_highlight"/>                        or                        <button string="Cancel" class="oe_link" special="cancel"/>                    </footer>                </form>            </field>        </record>        <act_window id="action_mrp_order_merge" name="Merge Production Order"                    res_model="mrp.order.merge" src_model="mrp.production"                    view_mode="form" multi="False" target="new" key2="client_action_multi"/>    </data></openerp>