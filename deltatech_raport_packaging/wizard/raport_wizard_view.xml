<odoo>
    <data>
        <record id="raport_packaging_wizard" model="ir.ui.view">
            <field name="name">Raport packaging wizard</field>
            <field name="model">raport_packaging_materials</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <footer>
                        <button name="do_raport" string="_Apply" type="object" class="oe_highlight" />
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
    <act_window
        id="raport_packaging_wizard"
        name="Raport packaging list"
        binding_model="account.move"
        res_model="raport_packaging_materials"
        view_mode="form"
        target="new"
    />
</odoo>
