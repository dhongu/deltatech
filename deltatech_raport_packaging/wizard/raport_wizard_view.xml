<odoo>
    <data>
        <record id="raport_packaging_wizard" model="ir.ui.view">
            <field name="name">Raport packaging wizard</field>
            <field name="model">raport_wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <footer>
                            <button name="raport_wizard" type="object" string="Raport wizard" class="oe_highlight"/>
                            <button special="cancel" string="Cancel"/>
                        </footer>
                    </sheet>

                </form>
            </field>
        </record>
    </data>
    <act_window
        id="raport_packaging_wizard"
        name="Raport packaging list"
        binding_model="account.move"
        res_model="raport_wizard"
        view_mode="form"
        target="new"
    />

</odoo>
