<?xml version="1.0" ?>
<odoo>
    <data>
        <record id="product_template_form_view" model="ir.ui.view">
            <field name="name">product.template.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="deltatech_sale_multiple.product_template_form_view" />
            <field name="arch" type="xml">
                <field name="qty_minim" position="after">
                    <field name="check_min_website" />
                </field>
            </field>
        </record>
        <record id="product_variant_easy_edit_view" model="ir.ui.view">
            <field name="name">product.product.form</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="deltatech_sale_multiple.product_variant_easy_edit_view" />
            <field name="arch" type="xml">
                <field name="qty_minim" position="after">
                    <field name="check_min_website" />
                </field>
            </field>
        </record>
    </data>
</odoo>
